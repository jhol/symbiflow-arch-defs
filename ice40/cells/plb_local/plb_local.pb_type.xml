<!-- ################################################################ -->
<!-- # Contains the array of 8 LUTFF elements                       # -->
<!-- ################################################################ -->
<pb_type name="plb_local" num_pb="1" xmlns:xi="http://www.w3.org/2001/XInclude">
 <input  name="cen"  num_pins="1" equivalent="false" />
 <clock  name="clk"  num_pins="1" equivalent="false" />
 <input  name="s_r"  num_pins="1" equivalent="false" />
 
 <input  name="in0"  num_pins="8" equivalent="true"/>
 <input  name="in1"  num_pins="8" equivalent="true"/>
 <input  name="in2"  num_pins="8" equivalent="true"/>
 <input  name="in3"  num_pins="8" equivalent="true"/>

 <output name="lout" num_pins="8" equivalent="true"/>
 <output name="out"  num_pins="8" equivalent="true"/>

 <output name="cout" num_pins="8" equivalent="false"/>
 <input  name="cin"  num_pins="1" equivalent="false"/>

 <mode name="ff">
  <xi:include href="../../cells/lutff/lutff.pb_type.xml"/>

  <interconnect>
   <!-- Global inputs -->
   <direct name="cen[0]" input="plb_local.cen" output="lutff[0].cen"/>
   <direct name="cen[1]" input="plb_local.cen" output="lutff[1].cen"/>
   <direct name="cen[2]" input="plb_local.cen" output="lutff[2].cen"/>
   <direct name="cen[3]" input="plb_local.cen" output="lutff[3].cen"/>
   <direct name="cen[4]" input="plb_local.cen" output="lutff[4].cen"/>
   <direct name="cen[5]" input="plb_local.cen" output="lutff[5].cen"/>
   <direct name="cen[6]" input="plb_local.cen" output="lutff[6].cen"/>
   <direct name="cen[7]" input="plb_local.cen" output="lutff[7].cen"/>

   <direct name="clk[0]" input="plb_local.clk" output="lutff[0].clk"/>
   <direct name="clk[1]" input="plb_local.clk" output="lutff[1].clk"/>
   <direct name="clk[2]" input="plb_local.clk" output="lutff[2].clk"/>
   <direct name="clk[3]" input="plb_local.clk" output="lutff[3].clk"/>
   <direct name="clk[4]" input="plb_local.clk" output="lutff[4].clk"/>
   <direct name="clk[5]" input="plb_local.clk" output="lutff[5].clk"/>
   <direct name="clk[6]" input="plb_local.clk" output="lutff[6].clk"/>
   <direct name="clk[7]" input="plb_local.clk" output="lutff[7].clk"/>

   <direct name="s_r[0]" input="plb_local.s_r" output="lutff[0].s_r"/>
   <direct name="s_r[1]" input="plb_local.s_r" output="lutff[1].s_r"/>
   <direct name="s_r[2]" input="plb_local.s_r" output="lutff[2].s_r"/>
   <direct name="s_r[3]" input="plb_local.s_r" output="lutff[3].s_r"/>
   <direct name="s_r[4]" input="plb_local.s_r" output="lutff[4].s_r"/>
   <direct name="s_r[5]" input="plb_local.s_r" output="lutff[5].s_r"/>
   <direct name="s_r[6]" input="plb_local.s_r" output="lutff[6].s_r"/>
   <direct name="s_r[7]" input="plb_local.s_r" output="lutff[7].s_r"/>

   <!-- LUTFF IO -->
   <direct name="in[0][0]" input="plb_local.in0[0]" output="lutff[0].in[0]"/>
   <direct name="in[0][1]" input="plb_local.in1[0]" output="lutff[0].in[1]"/>
   <direct name="in[0][2]" input="plb_local.in2[0]" output="lutff[0].in[2]"/>
   <direct name="in[0][3]" input="plb_local.in3[0]" output="lutff[0].in[3]"/>

   <direct name="in[1][0]" input="plb_local.in0[1]" output="lutff[1].in[0]"/>
   <direct name="in[1][1]" input="plb_local.in1[1]" output="lutff[1].in[1]"/>
   <direct name="in[1][2]" input="plb_local.in2[1]" output="lutff[1].in[2]"/>
   <direct name="in[1][3]" input="plb_local.in3[1]" output="lutff[1].in[3]"/>

   <direct name="in[2][0]" input="plb_local.in0[2]" output="lutff[2].in[0]"/>
   <direct name="in[2][1]" input="plb_local.in1[2]" output="lutff[2].in[1]"/>
   <direct name="in[2][2]" input="plb_local.in2[2]" output="lutff[2].in[2]"/>
   <direct name="in[2][3]" input="plb_local.in3[2]" output="lutff[2].in[3]"/>

   <direct name="in[3][0]" input="plb_local.in0[3]" output="lutff[3].in[0]"/>
   <direct name="in[3][1]" input="plb_local.in1[3]" output="lutff[3].in[1]"/>
   <direct name="in[3][2]" input="plb_local.in2[3]" output="lutff[3].in[2]"/>
   <direct name="in[3][3]" input="plb_local.in3[3]" output="lutff[3].in[3]"/>

   <direct name="in[4][0]" input="plb_local.in0[4]" output="lutff[4].in[0]"/>
   <direct name="in[4][1]" input="plb_local.in1[4]" output="lutff[4].in[1]"/>
   <direct name="in[4][2]" input="plb_local.in2[4]" output="lutff[4].in[2]"/>
   <direct name="in[4][3]" input="plb_local.in3[4]" output="lutff[4].in[3]"/>

   <direct name="in[5][0]" input="plb_local.in0[5]" output="lutff[5].in[0]"/>
   <direct name="in[5][1]" input="plb_local.in1[5]" output="lutff[5].in[1]"/>
   <direct name="in[5][2]" input="plb_local.in2[5]" output="lutff[5].in[2]"/>
   <direct name="in[5][3]" input="plb_local.in3[5]" output="lutff[5].in[3]"/>

   <direct name="in[6][0]" input="plb_local.in0[6]" output="lutff[6].in[0]"/>
   <direct name="in[6][1]" input="plb_local.in1[6]" output="lutff[6].in[1]"/>
   <direct name="in[6][2]" input="plb_local.in2[6]" output="lutff[6].in[2]"/>
   <direct name="in[6][3]" input="plb_local.in3[6]" output="lutff[6].in[3]"/>

   <direct name="in[7][0]" input="plb_local.in0[7]" output="lutff[7].in[0]"/>
   <direct name="in[7][1]" input="plb_local.in1[7]" output="lutff[7].in[1]"/>
   <direct name="in[7][2]" input="plb_local.in2[7]" output="lutff[7].in[2]"/>
   <direct name="in[7][3]" input="plb_local.in3[7]" output="lutff[7].in[3]"/>

   <direct name="lout[0]" input="lutff[0].lout" output="plb_local.lout[0]"/>
   <direct name="lout[1]" input="lutff[1].lout" output="plb_local.lout[1]"/>
   <direct name="lout[2]" input="lutff[2].lout" output="plb_local.lout[2]"/>
   <direct name="lout[3]" input="lutff[3].lout" output="plb_local.lout[3]"/>
   <direct name="lout[4]" input="lutff[4].lout" output="plb_local.lout[4]"/>
   <direct name="lout[5]" input="lutff[5].lout" output="plb_local.lout[5]"/>
   <direct name="lout[6]" input="lutff[6].lout" output="plb_local.lout[6]"/>
   <direct name="lout[7]" input="lutff[7].lout" output="plb_local.lout[7]"/>

   <direct name="out[0]" input="lutff[0].out" output="plb_local.out[0]"/>
   <direct name="out[1]" input="lutff[1].out" output="plb_local.out[1]"/>
   <direct name="out[2]" input="lutff[2].out" output="plb_local.out[2]"/>
   <direct name="out[3]" input="lutff[3].out" output="plb_local.out[3]"/>
   <direct name="out[4]" input="lutff[4].out" output="plb_local.out[4]"/>
   <direct name="out[5]" input="lutff[5].out" output="plb_local.out[5]"/>
   <direct name="out[6]" input="lutff[6].out" output="plb_local.out[6]"/>
   <direct name="out[7]" input="lutff[7].out" output="plb_local.out[7]"/>

   <!-- Carry chain -->
   <direct name="cin" input="plb_local.cin" output="lutff[0].cin"><pack_pattern name="CARRYCHAIN" in_port="plb_local.cin" out_port="lutff[0].cin"/></direct>

   <direct name="carry[0]" input="lutff[0].cout" output="lutff[1].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[0].cout" out_port="lutff[1].cin"     /></direct>
   <direct name="carry[1]" input="lutff[1].cout" output="lutff[2].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[1].cout" out_port="lutff[2].cin"     /></direct>
   <direct name="carry[2]" input="lutff[2].cout" output="lutff[3].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[2].cout" out_port="lutff[3].cin"     /></direct>
   <direct name="carry[3]" input="lutff[3].cout" output="lutff[4].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[3].cout" out_port="lutff[4].cin"     /></direct>
   <direct name="carry[4]" input="lutff[4].cout" output="lutff[5].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[4].cout" out_port="lutff[5].cin"     /></direct>
   <direct name="carry[5]" input="lutff[5].cout" output="lutff[6].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[5].cout" out_port="lutff[6].cin"     /></direct>
   <direct name="carry[6]" input="lutff[6].cout" output="lutff[7].cin"     ><pack_pattern name="CARRYCHAIN" in_port="lutff[6].cout" out_port="lutff[7].cin"     /></direct>
   <direct name="carry[7]" input="lutff[7].cout" output="plb_local.cout[7]"><pack_pattern name="CARRYCHAIN" in_port="lutff[7].cout" out_port="plb_local.cout[7]"/></direct>

   <direct name="cout[0]" input="lutff[0].cout" output="plb_local.cout[0]"/>
   <direct name="cout[1]" input="lutff[1].cout" output="plb_local.cout[1]"/>
   <direct name="cout[2]" input="lutff[2].cout" output="plb_local.cout[2]"/>
   <direct name="cout[3]" input="lutff[3].cout" output="plb_local.cout[3]"/>
   <direct name="cout[4]" input="lutff[4].cout" output="plb_local.cout[4]"/>
   <direct name="cout[5]" input="lutff[5].cout" output="plb_local.cout[5]"/>
   <direct name="cout[6]" input="lutff[6].cout" output="plb_local.cout[6]"/>
  </interconnect>

 </mode>
</pb_type>
